struct slam_state_t {
	int64_t utime;

  // x and y are coordinates in meters on the SLAM map
	double x;
	double y;

  // yaw is in radians relative to north on the SLAM map
	double yaw;

  // north angle that FOG is initialized to in radians
  boolean north_initialized;
  double north_angle;

  // latitude and longitude that the SLAM map is initialized with
  boolean lat_lon_initialized;
	double lat_origin;
	double lon_origin;
}
